html, body, div, ul, li, p {
  padding: 0;
  margin: 0;
}

ul li {
  list-style: none;
}

html {
  width: 100%;
  height: 100%;
}
html body {
  width: 100%;
  height: 100%;
}
html body header {
  width: 100%;
  height: 48px;
  background: #A90506;
  position: fixed;
  top: 0;
  text-align: center;
  line-height: 48px;
  color: #FFFffF;
  font-size: 24px;
}
html body header span {
  position: absolute;
  border: 1px solid #930E0C;
  right: 4%;
  height: 35px;
  width: 20%;
  top: 6px;
  line-height: 35px;
  border-radius: 10px;
  font-size: 18px;
}
html body nav {
  width: 92%;
  height: 48px;
  position: fixed;
  top: 48px;
  background: #484850;
  padding: 0 4%;
  line-height: 48px;
  color: #FFFFFF;
  font-size: 14px;
  font-weight: 600;
}
html body nav .allnum {
  display: inline-block;
  padding: 0 4% 0 1%;
}
html body nav .allpri {
  color: red;
  padding-left: 2%;
}
html body #contain {
  width: 100%;
  height: 100%;
  padding: 96px 0 55px 0;
  box-sizing: border-box;
  overflow: auto;
}
html body #contain section {
  width: 100%;
  height: 100%;
  overflow: auto;
}
html body #contain section ul {
  width: 100%;
  overflow: hidden;
}
html body #contain section ul li {
  width: 92%;
  padding: 2% 4%;
  border-top: 1px solid #A3A3A3;
  border-bottom: 1px solid #A3A3A3;
  overflow: hidden;
}
html body #contain section ul li .img {
  float: left;
  width: 30%;
  height: 90px;
}
html body #contain section ul li .img img {
  width: 100%;
  height: 100%;
}
html body #contain section ul li .info {
  float: left;
  width: 55%;
  padding: 0 3%;
  height: 100%;
}
html body #contain section ul li .info p {
  width: 100%;
  line-height: 21px;
}
html body #contain section ul li .info p .aprice {
  color: #A90506;
  font-weight: 600;
}
html body #contain section ul li .info p .cut {
  display: inline-block;
  border: 1px solid;
  vertical-align: middle;
  text-align: center;
  width: 15%;
  margin-right: 3%;
  border-radius: 5px;
  background: #808080;
  color: #FFFFFF;
  font-weight: 600;
}
html body #contain section ul li .info p .add {
  display: inline-block;
  border: 1px solid;
  vertical-align: middle;
  text-align: center;
  width: 15%;
  margin-left: 3%;
  border-radius: 5px;
  background: #808080;
  color: #FFFFFF;
  font-weight: 600;
}
html body #contain section ul li .info p input {
  text-align: center;
  width: 23%;
  height: 18px;
  border-radius: 2px;
  border: 1px solid #cccccc;
  outline: 0;
}
html body #contain section ul li .del {
  float: left;
  width: 6%;
  height: 100%;
  font-size: 22px;
  margin-left: 2%;
}
html body footer {
  width: 100%;
  height: 55px;
  position: fixed;
  bottom: 0;
  background: #484850;
  display: -webkit-box;
  color: #FFFFFF;
  z-index: 1;
}
html body footer span {
  border-top: 2px solid #484850;
  height: 100%;
  display: block;
  text-align: center;
  -webkit-box-flex: 1;
  padding-top: 4px;
}
html body footer span .i1, html body footer span .i2, html body footer span .i3, html body footer span .i4 {
  width: 100%;
  font-size: 30px;
  text-align: center;
}

